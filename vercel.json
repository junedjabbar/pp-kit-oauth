{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Authorization": "*",
                "Content-Type": "*",
                "X-Requested-With": "*",
                "Accept": "*",
                "Origin": "*",
                "X-Kit-Token": "*",
                "X-Custom-Header": "*"
            },
            "dest": "index.js"
        }
    ]
}